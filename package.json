{
  "name": "royal-order",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apis/*",
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "publish:prettier-config": "pnpm changeset version && pnpm changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@vanilla-extract/esbuild-plugin": "^2.2.2",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@8.1.1",
  "dependencies": {
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/sprinkles": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
